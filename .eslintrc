{
  "extends": ["react-native-prettier"],
  "rules": {
    "import/no-named-as-default": 0,
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "printWidth": 120,
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "trailingComma": "none",
        "semi": false
      }
    ]
  }
}
